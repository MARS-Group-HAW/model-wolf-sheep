{
  "globals": {
    "steps": 1000,
    "output": "csv",
    "options": {
      "delimiter": ";",
      "format": "en-EN"
    },
    "pythonVisualization": true,
    "console": true
  },
  "layers": [
    {
      "name": "GrasslandLayer",
      "file": "Resources/grid.csv"
    }
  ],
"agents": [
    {
      "name": "Sheep",
      "count": 30,
      "mapping": [
        {
          "parameter": "SheepGainFromFood",
          "value": 4
        },
        {
          "parameter": "SheepReproduce",
          "value": 5
        }
      ]
    },
    {
      "name": "Wolf",
      "count": 3,
      "mapping": [
        {
          "parameter": "WolfGainFromFood",
          "value": 20
        },
        {
          "parameter": "WolfReproduce",
          "value": 3
        }
      ]
    },
    {
      "name": "GrassGrowthAgent",
      "count": 1,
      "mapping": [
        {
          "parameter": "GrassRegrowthPerStep",
          "value": 1
        }
      ]
    }
  ]
}